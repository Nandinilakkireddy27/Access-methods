package basics;

public class Nonstatic {
	public static void staticmethod(int a, int b, int c) {
		System.out.println(a+b+c);
	}
	public void nonstaticmethod(int a, int b, int c) {
		System.out.println(a+b+c);
		Nonstatic.staticmethod(5,6,7);
	}

	public static void main(String[] args) {
		Nonstatic obj1=new Nonstatic();
		obj1.nonstaticmethod(2,3,4);

	}

}

